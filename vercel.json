{
    "version": 2,
    "builds": [
      {
        "src": "mainfolder/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "runtime": "python3.9",
          "buildCommand": "pip install -r requirements.txt"
        }
      },
      {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "staticfiles_build/public"
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
    },
      {
        "src": "/(.*)",
        "dest": "mainfolder/wsgi.py"
      }
    ]
  }
